{
  "schema": "http://json-schema.org/draft-07/schema#",
  "title": "verifyIban",
  "description": "Verifies a IBAN number",
  "type": "object",
  "properties": {
    "iban": {
      "type": "string",
      "description": "The IBAN number to verify",
      "pattern": "^[A-Z]{2}[0-9]{2}[A-Z0-9]{1,30}$"
    }
  },
  "required": [
    "iban"
  ]
}

